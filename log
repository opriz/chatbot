Q: 继续 
A: if(!left)
    {
        printf("\n");
        return 0;
    }
    int i;
    for(i=0;i<=9;i++)
    {
        if(left[i]>0)
        {
            left[i]--;
            print(left);
            left[i]++;
        }
    }
    return 0;
}

int main()
{
    int left[10];
    memset(left,0,10*sizeof(int));
    int i;
    for(i=0;i<5;i++)
    {
        scanf("%d",&left[i]);
    }
    print(left);
    return 0;
}
