Q: 帮写个手机可以玩得五子棋游戏代码 
A: // 五子棋游戏代码

//  定义变量
var board = []; // 棋盘
var me = true; // 默认先手
var over = false; // 游戏是否结束

// 初始化棋盘
for (var i = 0; i < 15; i++) {
	board[i] = [];
	for (var j = 0; j < 15; j++) {
		board[i][j] = 0;
	}
}

// 绘制棋盘
function drawChessBoard() {
	for (var i = 0; i < 15; i++) {
		ctx.moveTo(15 + i * 30, 15);
		ctx.lineTo(15 + i * 30, 435);
		ctx.stroke();
		ctx.moveTo(15, 15 + i * 30);
		ctx.lineTo(435, 15 + i * 30);
		ctx.stroke();
	}
}

// 落子
function oneStep(i, j, me) {
	ctx.beginPath();
	ctx.arc(15 + i * 30, 15 + j * 30, 13, 0, 2 * Math.PI);
	ctx.closePath();
	// 黑子
	var gradient = ctx.createRadialGradient(15 + i * 30 + 2, 15 + j * 30 - 2, 13, 15 + i * 30 + 2, 15 + j * 30 - 2, 0);
	if (me) {
		gradient.addColorStop(0, "#0A0A0A");
		gradient.addColorStop(1, "#636766");
	} else {
		gradient.addColorStop(0, "#D1D1D1");
		gradient.addColorStop(1, "#F9F9F9");
	}
	ctx.fillStyle = gradient;
	ctx.fill();
}

// 判断输赢
function judgeWin(i, j, me) {
	var count = 1; // 连续棋子数
	// 向右搜索
	for (var x = i + 1; x < 15; x++) {
		if (board[x][j] == me) {
			count++;
		} else {
			break;
		}
	}
	// 向左搜索
	for (var x = i - 1; x >= 0; x--) {
		if (board[x][j] == me) {
			count++;
		} else {
			break;
		}
	}
	if (count >= 5) {
		return true;
	}
	// 向上搜索
	count = 1;
	for (var y = j - 1; y >= 0; y--) {
		if (board[i][y] == me) {
			count++;
		} else {
			break;
		}
	}
	// 向下搜索
	for (var y = j + 1; y < 15; y++) {
		if (board[i][y] == me) {
			count++;
		} else {
			break;
		}
	}
	if (count >= 5) {
		return true;
	}
	// 向右上搜索
	count = 1;
	for (var x = i + 1, y = j - 1; x < 15 && y >= 0; x++, y--) {
		if (board[x][y] == me) {
			count++;
		} else {
			break;
		}
	}
	// 向左下搜索
	for (var x = i - 1, y = j + 1; x >= 0 && y
