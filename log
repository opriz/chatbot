Q: 注册
 
A: public function register(){
        if(IS_POST){
            // 实例化模型
            $user = D('User');
            // 自动验证
            if($user->create()){
                // 验证通过
                if($user->add()){
                    $this->success('注册成功',U('Index/index'));
                } else {
                    $this->error('注册失败');
                }
            } else {
                // 获取错误信息
                $this->error($user->getError());
            }
            return;
        }
        $this->display();
    }
    // 用户登录
    public function login(){
        if(IS_POST){
            // 实例化模型
            $user = D('User');
            // 自动验证
            if($user->validate($user->login_rule)->create()){
                // 验证通过
                if($user->login()){
                    $this->success('登录成功',U('Index/index'));
                } else {
                    $this->error('用户名或密码错误');
                }
            } else {
                // 获取错误信息
                $this->error($user->getError());
            }
            return;
        }
        $this->display();
    }
    // 用户退出
    public function logout(){
        session_unset();
        session_destroy();
        $this->success('退出成功',U('User/login'));
    }
}
>>>>>>>>>>分割线>>>>>>>>>>
