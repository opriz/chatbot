	$sql = "SELECT * FROM user WHERE username='$username' AND password='$password'";
	
	$res = mysqli_query($conn,$sql);
	
	$row = mysqli_fetch_assoc($res);
	
	if($row){
		echo 1;
		//header('location:../index.html');
		
