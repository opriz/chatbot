Q: 用C++编程语言写一个快速排序代码 
A: ```cpp
#include <iostream>
using namespace std;

// 快速排序函数
void quicksort(int arr[], int left, int right)
{
	int i = left;
	int j = right;
	int temp;
	int pivot = arr[(left + right) / 2];

	// 将比pivot小的放到左边，大的放到右边
	while (i <= j) {
		while (arr[i] < pivot)
			i++;
		while (arr[j] > pivot)
			j--;
		if (i <= j) {
			temp = arr[i];
			arr[i] = arr[j];
			arr[j] = temp;
			i++;
			j--;
		}
	}

	// 递归排序左边
	if (left < j)
		quicksort(arr, left, j);
	// 递归排序右边
	if (i < right)
		quicksort(arr, i, right);
}

int main()
{
	int arr[] = { 5, 7, 3, 8, 4, 9, 1, 6, 2 };
	quicksort(arr, 0, 8);
	for (int i = 0; i < 9; i++)
		cout << arr[i] << " ";
	cout << endl;
	return 0;
}
```
