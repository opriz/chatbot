Q: 写一组贪吃蛇的代码 
A: // 定义贪吃蛇的坐标
var snake = [
    {x: 0, y: 0}, 
    {x: 0, y: 1}, 
    {x: 0, y: 2}, 
    {x: 0, y: 3}
];

// 定义贪吃蛇移动的方向
var direction = 'right';

// 定义每次移动的增量
var stepX = 1;
var stepY = 0;

// 定义贪吃蛇的移动函数
function move() {
    // 获取贪吃蛇的头部
    var head = snake[snake.length - 1];

    // 根据当前的移动方向，计算新的坐标
    var newX = head.x + stepX;
    var newY = head.y + stepY;

    // 将新的坐标添加到贪吃蛇数组中
    snake.push({x: newX, y: newY});

    // 移除贪吃蛇数组中的第一个元素
    snake.shift();
}
