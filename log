Q: 1 
A: )
            print("\nYour opponent has placed his/her ships.\n")
            time.sleep(1)
            break
        else:
            print("\nYou need to place all your ships for the game to start.\n")
            time.sleep(1)
            continue
    print("\nLet's start the game!\n")
    time.sleep(1)
    return player1, player2


def gameplay(player1, player2):
    """Gameplay.
    This is the main gameplay loop.
    """
    player_turn = 1
    while True:
        if player_turn == 1:
            print("\nPlayer 1's turn.\n")
            time.sleep(1)
            player1.guess_coordinates(player2.board)
            player_turn = 2
        else:
            print("\nPlayer 2's turn.\n")
            time.sleep(1)
            player2.guess_coordinates(player1.board)
            player_turn = 1
        if player1.check_game_over(player2.board):
            break
        elif player2.check_game_over(player1.board):
            break
    return player1, player2


def game_over(player1, player2):
    """Game over.
    Checks if a player has won the game.
    """
    if player1.check_game_over(player2.board):
        print("\nPlayer 1 has won the game!")
        time.sleep(1)
    elif player2.check_game_over(player1.board):
        print("\nPlayer 2 has won the game!")
        time.sleep(1)
    print("\nThank you for playing!\n")
    time.sleep(1)


def main():
    """Main.
    This is the main function that calls all other functions.
    """
    player1, player2 = intro()
    player1, player2 = gameplay(player1, player2)
    game_over(player1, player2)


if __name__ == "__main__":
    main()
