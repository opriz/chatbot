Q: 用Java实现一个webAPI 
A: import java.io.*;
import java.net.*;
import java.util.*;
import javax.servlet.*;

public class WebAPI {
	public static void main(String[] args) throws Exception {
		String httpMethod = "GET";
		String url = "http://example.com/api";
		String queryString = "name=John&age=20";
		
		URL requestUrl = new URL(url + "?" + queryString);
		HttpURLConnection connection = (HttpURLConnection) requestUrl.openConnection();
		connection.setRequestMethod(httpMethod);
		
		int responseCode = connection.getResponseCode();
		if (responseCode == 200) {
			InputStream responseStream = connection.getInputStream();
			BufferedReader reader = new BufferedReader(new InputStreamReader(responseStream));
			String line;
			while ((line = reader.readLine()) != null) {
				System.out.println(line);
			}
			reader.close();
		}
		connection.disconnect();
	}
}
>>>>>>>>>>分割线>>>>>>>>>>
Q: 用c实现一个webAPI 
A: #include <stdio.h>
#include <string.h>
#include <stdlib.h>

typedef struct {
    char *name;
    char *value;
} Param;

typedef struct {
    char *method;
    char *path;
    Param *params;
    int params_len;
} Request;

typedef struct {
    int status;
    char *content;
} Response;

Request *parse_request(char *raw) {
    Request *request = malloc(sizeof(Request));
    request->params = malloc(sizeof(Param) * 10);
    request->params_len = 0;

    char *p = strtok(raw, " ");
    request->method = malloc(strlen(p) + 1);
    strcpy(request->method, p);

    p = strtok(NULL, " ");
    request->path = malloc(strlen(p) + 1);
    strcpy(request->path, p);

    p = strtok(NULL, " ");
    if (strcmp(p, "HTTP/1.1") != 0) {
        return NULL;
    }

    p = strtok(NULL, "\n");
    while (p != NULL) {
        Param *param = request->params + request->params_len;
        param->name = malloc(strlen(p) + 1);
        param->value = malloc(strlen(p) + 1);
        sscanf(p, "%[^=]=%s", param->name, param->value);
        request->params_len++;
        p = strtok(NULL, "\n");
    }

    return request;
}

Response *handle_request(Request *request) {
    Response *response = malloc(sizeof(Response));
    response->status = 200;

    if (strcmp(request->method, "GET") == 0) {
        if (strcmp(request->path, "/hello") == 0) {
            char *content = malloc(100);
            sprintf(content, "hello %s", request->params[0].value);
            response->content = content;
        }
    }

    return response;
}

void free_request(Request *request) {
    free(request->method);
    free(request->path);
    for (int i = 0; i < request->params_len; i++) {
        Param *param = request->params + i;
        free(param->name);
        free(param->value);
    }
    free(request->params);
    free(request);
}

void free_response(Response *response) {
    free(response->content);
    free(response);
}

int main() {
    char *raw = "GET /hello?name=jack HTTP/1.1\nHost: localhost\n";
    Request *request = parse_request(raw);
    Response *response = handle_request(request);
    printf("status: %d\ncontent: %s\n", response->status, response->content);
    free_request(request);
    free_response(response);
    return 0;
}
