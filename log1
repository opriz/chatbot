Q: stm32点亮led程序 
A: //STM32点亮LED程序
#include "stm32f10x.h"

int main(void)
{
    //1.使能时钟
    RCC->APB2ENR |= 1<<3;//使能PORTB时钟
    //2.设置端口模式
    GPIOB->CRL &= 0XFFFFFFF0;//复位
    GPIOB->CRL |= 0X00000003;//PB0输出推挽
    //3.设置端口输出
    GPIOB->ODR |= 1<<0;//PB0输出高
    while(1);
    return 0;
}
