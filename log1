Q: continue 
A: }
				}
				// If this is a new item, add it to the list
				if !exists {
					c.items = append(c.items, item)
				}
				// Now, add the item to the list of items to be added to the
				// container.
				items = append(items, item)
			}
		}
	}

	// Now that we have a list of items to add to the container, let's add
	// them.
	for _, item := range items {
		err := c.addItem(ctx, item)
		if err != nil {
			return err
		}
	}

	return nil
}

// addItem adds an item to the container. This is a helper function for
// addItems.
func (c *Container) addItem(ctx context.Context, item *Item) error {
	// First, we need to check if the item already exists in the container.
	// If it does, we don't need to add it.
	if c.hasItem(item) {
		return nil
	}

	// Next, we need to check if the item is valid.
	err := c.validateItem(ctx, item)
	if err != nil {
		return err
	}

	// Now, add the item to the container.
	c.items = append(c.items, item)

	return nil
}

// removeItems removes a list of items from the container.
func (c *Container) removeItems(ctx context.Context, items []*Item) error {
	// First, check that all the items are valid.
	for _, item := range items {
		err := c.validateItem(ctx, item)
		if err != nil {
			return err
		}
	}

	// Next, remove the items from the container.
	for _, item := range items {
		err := c.removeItem(ctx, item)
		if err != nil {
			return err
		}
	}

	return nil
}

// removeItem removes an item from the container. This is a helper
// function for removeItems.
func (c *Container) removeItem(ctx context.Context, item *Item) error {
	// First, check if the item exists in the container.
	index := c.indexOfItem(item)
	if index == -1 {
		return ErrItemNotFound
	}

	// Now, remove the item from the container.
	c.items = append(c.items[:index], c.items[index+1:]...)

	return nil
}

// moveItems moves a list of items from one container to another.
func (c *Container) moveItems(ctx context.Context, items []*Item, dest *Container) error {
	// First, check that all the items are valid.
	for _, item := range items {
		err := c.validateItem(ctx, item)
		if err != nil {
			return err
		}
	}

	// Next, move the items from one container to the other.
	for _, item := range items {
		err := c.moveItem(ctx, item, dest)
		if err != nil {
			return err
		}
	}

	return nil
}

// moveItem moves an item from one container to another. This is a helper
// function for moveItems.
func (c *Container) moveItem(ctx context.Context, item *Item, dest *Container) error {
	// First, check if the item exists in the source container.
	index := c.indexOfItem(item)
	if index == -1 {
		return ErrItemNotFound
	}

	// Next, check if the item already exists in the destination container.
	// If it does, we don't need to move it.
	if dest.hasItem(item) {
		return nil
	}

	// Now, remove the item from the source container.
	c.items = append(c.items[:index], c
>>>>>>>>>>分割线>>>>>>>>>>
