Q: 仪表卡次数错误 
A: private String meterError;
    //抄表异常
    private String readError;
    //抄表失败
    private String readFail;
    //抄表成功
    private String readSuccess;
    //抄表率
    private String readRate;
    //抄表总数
    private String total;
    //未抄数
    private String noread;
    //抄表成功率
    private String succRate;


    public String getMeterError() {
        return meterError;
    }

    public void setMeterError(String meterError) {
        this.meterError = meterError;
    }

    public String getReadError() {
        return readError;
    }

    public void setReadError(String readError) {
        this.readError = readError;
    }

    public String getReadFail() {
        return readFail;
    }

    public void setReadFail(String readFail) {
        this.readFail = readFail;
    }

    public String getReadSuccess() {
        return readSuccess;
    }

    public void setReadSuccess(String readSuccess) {
        this.readSuccess = readSuccess;
    }

    public String getReadRate() {
        return readRate;
    }

    public void setReadRate(String readRate) {
        this.readRate = readRate;
    }

    public String getTotal() {
        return total;
    }

    public void setTotal(String total) {
        this.total = total;
    }

    public String getNoread() {
        return noread;
    }

    public void setNoread(String noread) {
        this.noread = noread;
    }

    public String getSuccRate() {
        return succRate;
    }

    public void setSuccRate(String succRate) {
        this.succRate = succRate;
    }
}
