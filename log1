            if(b[i][0]==b[i][1]&&b[i][1]==b[i][2]&&b[i][2]==2)
            {
                flag=1;
                printf("Computer Win!\n");
                break;
            }
            if(b[0][i]==b[1][i]&&b[1][i]==b[
