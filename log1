A: /// </summary>
        /// <param name="teaPriceModel"></param>
        /// <returns></returns>
        public int UpdateTeaPrice(TeaPriceModel teaPriceModel)
        {
            var teaPriceDal = new TeaPriceDAL();
            return teaPriceDal.UpdateTeaPrice(teaPriceModel);
        }

        /// <summary>
        /// 获取所有茶价格
        /// </summary>
        /// <returns></returns>
        public List<TeaPriceModel> GetAllTeaPrice()
        {
            var teaPriceDal = new TeaPriceDAL();
            return teaPriceDal.GetAllTeaPrice();
        }
    }
}
