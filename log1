Q: 给我一个mybit-plus配置文件demo 
A: // MyBit-Plus Configuration File

// General Settings
//------------------------------------------------------------------------------

// Site name
site_name = "MyBit-Plus"

// Site URL
site_url = "https://www.mybit-plus.com"

// Site logo
site_logo = "/path/to/logo.png"

// Enable registration
enable_registration = true

// Enable two factor authentication
enable_2fa = true

// Enable CAPTCHA
enable_captcha = true

// Enable email verification
enable_email_verification = true

// Enable IP whitelisting
enable_ip_whitelisting = false

// Admin Settings
//------------------------------------------------------------------------------

// Admin email
admin_email = "admin@mybit-plus.com"

// Admin username
admin_username = "admin"

// Admin password
admin_password = "password"

// Database Settings
//------------------------------------------------------------------------------

// Database host
db_host = "localhost"

// Database port
db_port = 3306

// Database name
db_name = "mybit_plus_db"

// Database username
db_username = "mybit_plus_user"

// Database password
db_password = "password"
