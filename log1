Q: 一个单片机可用的环形缓冲区代码 
A: #include "main.h"

#define BUF_SIZE 4

uint8_t buf[BUF_SIZE]; //缓冲区
uint8_t head = 0;   //头指针
uint8_t tail = 0;   //尾指针

//缓冲区是否为空
uint8_t buf_is_empty(void)
{
    if(head == tail)
        return 1;
    else
        return 0;
}

//缓冲区是否已满
uint8_t buf_is_full(void)
{
    if(head == (tail + 1) % BUF_SIZE)
        return 1;
    else
        return 0;
}

//入队
void buf_enqueue(uint8_t data)
{
    if(!buf_is_full())
    {
        buf[tail] = data;
        tail = (tail + 1) % BUF_SIZE;
    }
}

//出队
uint8_t buf_dequeue(void)
{
    uint8_t data = 0;
    if(!buf_is_empty())
    {
        data = buf[head];
        head = (head + 1) % BUF_SIZE;
    }
    return data;
}
