Q: 给我写段对弈代码 
A: #include <iostream>
using namespace std;
 
int main()
{
    int playerA = 0;
    int playerB = 0;
 
    while (playerA != 3 && playerB != 3)
    {
        int resultA = 0, resultB = 0;
        cout << "Player A: ";
        cin >> resultA;
        cout << "Player B: ";
        cin >> resultB;
 
        if (resultA == resultB)
        {
            cout << "Tie!" << endl;
        }
        else if (resultA == 1 && resultB == 0)
        {
            cout << "Player A wins!" << endl;
            playerA++;
        }
        else if (resultA == 0 && resultB == 1)
        {
            cout << "Player B wins!" << endl;
            playerB++;
        }
        else
        {
            cout << "Invalid input!" << endl;
        }
    }
 
    if (playerA == 3)
    {
        cout << "Player A is the winner!" << endl;
    }
    else
    {
        cout << "Player B is the winner!" << endl;
    }
 
    return 0;
}
