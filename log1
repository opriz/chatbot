        int index = -1;
        // 遍历集合
        for (int i = 0; i < list.size(); i++) {
            TuSu ts = list.get(i);
            if (name.equals(ts.getName())) {
                index = i;
                break;
            }
        }
        if (index == -1) {
            System.out.println("没有找到该屠苏！");
        } else {
            System.out.println("请输入新的屠苏名称：");
            String newName = sc.next();
            System.out.println("请输入新的屠苏价格：");
            double newPrice = sc.nextDouble();
            TuSu ts = new TuSu(newName, newPrice);
            list.set(index, ts);
            System.out.println("修改屠苏成功
