Q: 查询天气
 
A: if (message.indexOf("天气") != -1) {
                var city = message.substring(0, message.indexOf("天气"));
                var url = "https://free-api.heweather.net/s6/weather/now?location=" + city + "&key=4b4d4b9d93464f09bfd6b7a6b22fc6b2";
                $.getJSON(url, function(data) {
                    var weather = data.HeWeather6[0].now.cond_txt;
                    var temp = data.HeWeather6[0].now.tmp;
                    output = city + "的天气是" + weather + "，温度是" + temp + "℃";
                    showMessage(output, "robot");
                });
            }
            //查询快递
            else if (message.indexOf("快递") != -1) {
                var express = message.substring(0, message.indexOf("快递"));
                var url = "https://www.kuaidi100.com/query?type=" + express + "&postid=11111111111&temp=0.5185995878803349";
                $.getJSON(url, function(data) {
                    var status = data.state;
                    var com = data.com;
                    output = "快递" + express + "的状态是" + status + ",快递公司是" + com;
                    showMessage(output, "robot");
                });
            }
            //查询翻译
            else if (message.indexOf("翻译") != -1) {
                var word = message.substring(0, message.indexOf("翻译"));
                var url = "http://fanyi.youdao.com/openapi.do?keyfrom=skyline-fanyi&key=2058911035&type=data&doctype=json&version=1.1&q=" + word;
                $.getJSON(url, function(data) {
                    var translation = data.translation;
                    output = word + "的翻译是" + translation;
                    showMessage(output, "robot");
                });
            }
            //查询音乐
            else if (message.indexOf("音乐") != -1) {
                var music = message.substring(0, message.indexOf("音乐"));
                var url = "https://api.imjad.cn/cloudmusic/?type=search&search_type=1&s=" + music;
                $.getJSON(url, function(data) {
                    var song = data.result.songs[0];
                    output = "歌曲：" + song.name + "<br>" + "歌手：" + song.ar[0].name + "<br>" + "专辑：" + song.al.name;
                    showMessage(output, "robot");
                });
            }
            //回答问题
            else {
                if (message.indexOf("你叫什么名字") != -1) {
                    output = "我叫小智，很高兴认识你！";
                } else if (message.indexOf("你多少岁") != -1) {
                    output = "我今年1岁，不算老也不算小！";
                } else if (message.indexOf("你是谁") != -1) {
                    output = "我是小智，一个智能机器人！";
                } else if (message.indexOf("你会什么") != -1) {
                    output = "我会回答
