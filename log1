Q: good 
A: s_total_price' => $goods_total_price,
            'order_total_price' => $order_total_price,
            'goods_data' => $goods_data,
            'address_data' => $address_data,
            'pay_data' => $pay_data,
            'delivery_data' => $delivery_data,
            'order_data' => $order_data
        ]);
    }

    public function order_pay(){
        // 订单id
        $order_id = request()->post('order_id');
        // 支付方式
        $pay_type = request()->post('pay_type');
        // 订单信息
        $order_data = OrderModel::get($order_id);
        // 订单金额
        $order_total_price = $order_data['order_total_price'];
        // 支付金额
        $pay_price = $order_total_price;
        // 支付时间
        $pay_time = time();
        // 支付状态
        $pay_status = 2;
        // 支付方式
        $pay_type = $pay_type;
        // 更改订单状态
        $order_data->pay_status = $pay_status;
        $order_data->pay_type = $pay_type;
        $order_data->pay_price = $pay_price;
        $order_data->pay_time = $pay_time;
        $res = $order_data->save();
        if($res){
            // 修改库存
            $this->changeStock($order_id);
            // 删除购物车
            $this->delCart($order_id);
            return json(['code' => 200, 'message' => '支付成功']);
        }else{
            return json(['code' => 400, 'message' => '支付失败']);
        }
    }

    public function changeStock($order_id){
        // 根据订单id查询购物车信息
        $cart_data = CartModel::alias('c')->join('order_goods og', 'c.goods_id = og.goods_id')->where(['order_id' => $order_id])->field('og.goods_num, c.goods_id')->select();
        // 循环修改库存
        foreach($cart_data as $key => $value){
            // 根据商品id查询商品信息
            $goods_data = GoodsModel::where(['goods_id' => $value['goods_id']])->field('goods_num, goods_sales')->find();
            // 构造修改条件
            $update_data['goods_num'] = $goods_data['goods_num'] - $value['goods_num'];
            $update_data['goods_sales'] = $goods_data['goods_sales'] + $value['goods_num'];
            // 执行修改
            GoodsModel::where(['goods_id' => $value['goods_id']])->update($update_data);
        }
    }

    public function delCart($order_id){
        // 根据订单id查询购物车信息
        $cart_data = CartModel::alias('c')->join('order_goods og', 'c.goods_id = og.goods_id')->where(['order_
>>>>>>>>>>分割线>>>>>>>>>>
