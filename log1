Q: 用c语言设计一个游戏 
A: #include <stdio.h>
#include <stdlib.h>
#include <time.h>

int main()
{
    int i, j, k;
    int r, c;
    int x, y;
    int count = 0;
    int flag = 0;
    int map[9][9];
    srand((unsigned)time(NULL));

    /* 初始化地图 */
    for (i = 0; i < 9; i++)
    {
        for (j = 0; j < 9; j++)
        {
            map[i][j] = 0;
        }
    }

    /* 随机生成10个障碍物 */
    for (k = 0; k < 10; k++)
    {
        r = rand() % 9;
        c = rand() % 9;
        map[r][c] = 1;
    }

    /* 打印地图 */
    for (i = 0; i < 9; i++)
    {
        for (j = 0; j < 9; j++)
        {
            if (map[i][j] == 0)
            {
                printf("-");
            }
            else
            {
                printf("*");
            }
        }
        printf("\n");
    }

    /* 设置初始位置 */
    x = 0;
    y = 0;
    map[x][y] = 2;
    printf("你的初始位置是：(%d, %d)\n", x, y);

    /* 游戏开始 */
    while (flag == 0)
    {
        printf("请输入上下左右的移动方向：\n");
        char dir;
        scanf("%c", &dir);
        switch (dir)
        {
        case 'w':
            x--;
            break;
        case 's':
            x++;
            break;
        case 'a':
            y--;
            break;
        case 'd':
            y++;
            break;
        default:
            printf("输入错误！\n");
            break;
        }

        /* 位置有效性检查 */
        if (x < 0 || x > 8 || y < 0 || y > 8)
        {
            printf("超出边界！\n");
            x = (x + 9) % 9;
            y = (y + 9) % 9;
            printf("你的位置：(%d, %d)\n", x, y);
        }
        else
        {
            /* 判断移动后的位置是否为障碍物 */
            if (map[x][y] == 0)
            {
                map[x][y] = 2;
                printf("你的位置：(%d, %d)\n", x, y);
            }
            else
            {
                printf("移动到障碍物！\n");
            }
        }

        /* 判断是否到达终点 */
        if (x == 8 && y == 8)
        {
            printf("恭喜你，获得胜利！\n");
            flag = 1;
        }
        else
        {
            count++;
            printf("你已经走了%d步\n", count);
        }
    }

    return 0;
}
