								if (readBufferLength >= CMD_LEN) {
									// 将缓存中的数据拷贝到cmdBuffer中，因为cmdBuffer是全局变量，在接收线程中不能直接使用
									for (int i = 0; i < CMD_LEN; i++) {
										cmdBuffer[i] = readBuffer[i];
									}
									// 将缓存中剩余的数据往前移动
									for (int i = CMD_LEN; i < readBufferLength; i++) {
										readBuffer[i - CMD_LEN] = readBuffer[i];
									}
									readBufferLength -= CMD_LEN;
									// 将缓存中的数据清零
									for (int i = readBufferLength; i < CMD_LEN; i++) {
										readBuffer[i] = 0;
									}
									// 根据帧头判断是不是一帧完整的数据
									if (cmdBuffer[0] == 0xAA && cmdBuffer[1] == 0x55) {
										// 读取设备类型
										int deviceType = cmdBuffer[2] & 0xFF;
										// 读取功能码
										int funCode = cmdBuffer[3] & 0xFF;
										// 读取数据长度
										int dataLen = cmdBuffer[4] & 0xFF;
										// 读取数据
										byte[] dataBuffer = new byte[dataLen];
										for (int i = 0; i < dataLen; i++) {
											dataBuffer[i] = cmdBuffer[5 + i];
